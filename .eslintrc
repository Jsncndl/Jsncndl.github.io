{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "warn",
    "import/order": ["error", { "groups": ["builtin", "external", "internal"], "newlines-between": "always" }]
  }
}
